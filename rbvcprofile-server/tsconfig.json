{
  "compilerOptions": {
    "skipLibCheck": true,
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build",
  },
  "include": ["*"],
  // "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
